window.REBELMOUSE_TASKS_QUEUE.push(function(){

    
        function ChangeListener(selector, callback) {
    const initializedNodes = new Set([]);
    document.querySelectorAll(selector).forEach(node => {
        callback(node);
        initializedNodes.add(node);
    });
    const mutationObserver = new MutationObserver(() => {
        const nodes = document.querySelectorAll(selector);
        nodes.forEach(node => {
            if (!initializedNodes.has(node)) {
                callback(node);
                initializedNodes.add(node);
            }
        });
    });
    mutationObserver.observe(document.body, {
        childList: true,
        subtree: true
    });
}

ChangeListener(".widget .social-date__text, .post-date", function (date) {
	let date_string = date.innerHTML;
	date.classList.add('processed');
	let Months = {'January':'Janeiro','February':'Fevereiro','March':'Mar\u00e7o','April':'Abril','May':'Maggio','June':'Junho','July':'Julho','August':'Agosto','September':'Setembro','October':'Outubro','November':'Novembro','December':'Dezembro'};
	var Reg = new RegExp(Object.keys(Months).join("|"),"gi");
	date_string = date_string.replace(Reg, function(matched){
		return Months[matched];
		});
	date.innerHTML = date_string;
});
    

});

